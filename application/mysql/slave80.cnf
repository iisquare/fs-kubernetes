[mysqld]
server-id=80
skip-log-bin

wait_timeout=28800
interactive_timeout=28800
net_read_timeout=30
net_write_timeout=60
lock_wait_timeout=60
max_connections=2000
long_query_time=2
slow-query-log=on
log_queries_not_using_indexes=off
slow-query-log-file=/var/lib/mysql/slow-query.log
log-queries-not-using-indexes
